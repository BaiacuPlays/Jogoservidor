{
  "framework": "nextjs",
  "crons": [
    {
      "path": "/api/scheduled/generate-mixes",
      "schedule": "0 0 * * *"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "regions": ["gru1"]
}